root ::= object
sep ::= [ \t\n]*
object ::= string | integer | boolean | list | map | null
string ::= "\"" ("\\" [^] | [^"])+ "\""
integer ::= "-"? [0-9]+
decimal ::= "-"? [0-9]+ "." [0-9]+
boolean ::= "true" | "false"
list ::= sep  "[" sep (object sep ("," sep object)*)? sep "]" sep
map ::= "{" sep (mapEntry sep ("," sep mapEntry)*)? sep "}" sep
mapEntry ::= string sep ":" sep object
null ::= "null"